pipeline {
  agent any
  stages {
    stage('Clone') {
      steps {
        git url: 'https://github.com/your-username/your-repo.git'
      }
    }
    stage('Build') {
      steps {
        sh 'javac HelloWorld.java'
      }
    }
    stage('Test') {
      steps {
        sh 'java HelloWorld'
      }
    }
  }
}

